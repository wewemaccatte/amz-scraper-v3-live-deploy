{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/product(.*)",
      "dest": "app.js"
    },
    {
      "src": "/api/search(.*)",
      "dest": "app.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}
